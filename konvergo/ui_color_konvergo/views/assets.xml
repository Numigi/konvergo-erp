<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="_assets_primary_variables" name="Colors" inherit_id="web._assets_primary_variables">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/ui_color_konvergo/static/src/sass/colors.scss"/>
        </xpath>
    </template>

	<template id="assets_backend" name="Backend Assets" inherit_id="web.assets_backend">
		<xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/ui_color_konvergo/static/src/sass/navbar.scss"/>
        </xpath>
    </template>

    <template id="_assets_backend_helpers" name="Variables" inherit_id="web._assets_backend_helpers" priority="1">
        <xpath expr="//link[@href='/web/static/src/scss/bootstrap_overridden.scss']" position="after">
            <link rel="stylesheet" type="text/scss" href="/ui_color_konvergo/static/src/sass/variables.scss"/>
        </xpath>
    </template>

</odoo>
